<?xml version="1.0"?>
<root>

  <item>
    <name>Change the eject key to "Move to Trash" in Finder.</name>
    <identifier>private.finder_eject_to_move_to_trash</identifier>
    <only>FINDER</only>
    <uielementrole_not>{{ UIELEMENTROLES_EDITING_IN_FINDER }}</uielementrole_not>
    <autogen>
      __KeyToKey__
      ConsumerKeyCode::EJECT, ModifierFlag::NONE,
      KeyCode::DELETE, ModifierFlag::COMMAND_L,
    </autogen>
  </item>

  <item>
    <name>Change the eject key to forward delete key.</name>
    <identifier>private.eject_to_forward_delete</identifier>
    <autogen>
      __KeyToKey__
      ConsumerKeyCode::EJECT,
      KeyCode::FORWARD_DELETE,
    </autogen>
  </item>

</root>
