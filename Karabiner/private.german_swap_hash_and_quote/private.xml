<?xml version="1.0"?>
<root>

  <item>
    <name>Swap # and ' in German input source.</name>
    <identifier>private.german_swap_hash_and_quote</identifier>

    <inputsource_only>GERMAN</inputsource_only>

    <autogen>
      __KeyToKey__
      KeyCode::BACKSLASH, ModifierFlag::NONE,
      KeyCode::BACKSLASH, ModifierFlag::SHIFT_L,
    </autogen>

    <autogen>
      __KeyToKey__
      KeyCode::BACKSLASH, MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_SHIFT | ModifierFlag::NONE,
      KeyCode::BACKSLASH,
    </autogen>
  </item>

</root>
