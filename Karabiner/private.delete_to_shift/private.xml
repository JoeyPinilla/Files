<?xml version="1.0"?>
<root>

  <item>
    <name>Change delete key to shift key</name>
    <identifier>private.delete_to_shift</identifier>

    <autogen>
      __KeyToKey__
      KeyCode::DELETE,
      KeyCode::SHIFT_L,
    </autogen>
  </item>

  <item>
    <name>Change the right command key to tab key</name>
    <identifier>private.command_r_to_tab</identifier>

    <autogen>
      __KeyToKey__
      KeyCode::COMMAND_R,
      KeyCode::TAB,
    </autogen>
  </item>

</root>
