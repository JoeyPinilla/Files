    <autogen>
      __KeyOverlaidModifier__
      <!-- from -->
      KeyCode::BACKSLASH,
      <!-- to -->
      KeyCode::FN,
      <!-- alone -->
      KeyCode::BACKSLASH,
    </autogen>
  </item>

