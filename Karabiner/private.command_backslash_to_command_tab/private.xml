<?xml version="1.0"?>
<root>

  <item>
    <name>Change command-| to command-tab</name>
    <identifier>private.command_backslash_to_command_tab</identifier>

    <modifier_not>
      ModifierFlag::CONTROL_L,
      ModifierFlag::CONTROL_R,
      ModifierFlag::FN,
      ModifierFlag::OPTION_L,
      ModifierFlag::OPTION_R,
    </modifier_not>
    <autogen>
      __KeyToKey__
      KeyCode::BACKSLASH, MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_COMMAND,
      KeyCode::TAB, MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_COMMAND,
    </autogen>
  </item>

</root>
