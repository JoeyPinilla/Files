<?xml version="1.0"?>
<root>

  <item>
    <name>Change f15 and f16 to brackets.</name>
    <identifier>private.f15_f16_to_brackets</identifier>

    <autogen>
      __KeyToKey__
      KeyCode::F15,
      KeyCode::KEY_9, ModifierFlag::SHIFT_L,
    </autogen>
    <autogen>
      __KeyToKey__
      KeyCode::F16,
      KeyCode::KEY_0, ModifierFlag::SHIFT_L,
    </autogen>
  </item>

</root>
