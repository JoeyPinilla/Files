<?xml version="1.0"?>
<root>

  <item>
    <name>Fix bouncing E key</name>
    <identifier>private.bouncing_e</identifier>

    <lastpressedphysicalkey_only>KeyCode::E</lastpressedphysicalkey_only>
    <elapsedtimesincelastpressed_lessthan>Millisecond::RawValue::500</elapsedtimesincelastpressed_lessthan>

    <autogen>__KeyToKey__ KeyCode::E, KeyCode::VK_NONE</autogen>
  </item>

  <item>
    <name>Change F4 to X, X to E</name>
    <identifier>private.bouncing_e_2</identifier>

    <autogen>__KeyToKey__ KeyCode::F4, KeyCode::X</autogen>
    <autogen>__KeyToKey__ KeyCode::LAUNCHPAD, KeyCode::X</autogen>

    <autogen>__KeyToKey__ KeyCode::X, KeyCode::E</autogen>
    <autogen>__KeyToKey__ KeyCode::E, KeyCode::VK_NONE</autogen>
  </item>

</root>
